/* 交办任务 */
.assigned-task{
  width: 100%;
  height: 100%;
  position: relative;
  background: #f6f8fb;
}
.assigned-task>.content-top{
  position: relative;
  width: 100%;
  top: 0;
  left: 0;
  z-index: 999;
  height: 2.2rem;
  padding: 0 0 .1rem 0;
}
.assigned-task>.content-top>.area-head{
  height: .56rem;
  width: 100%;
  text-align: center;
  position: relative;
  padding: .17rem;
  font-size: .16rem;
  font-weight: bold;
  background: #fff;
}
.assigned-task>.content-top>.area-head>p>i{
  width: .18rem;
  height: .18rem;
  position: absolute;
  left: .26rem;
  top: .18rem;
  background: url("../images/back.png") no-repeat center;
  background-size: 100% 100%;
}
.assigned-task>.content-top>.searche-box{
  width: 100%;
  height: .33rem; 
  padding: 0 0.2rem;
  position: relative;
  background: #fff;
}
.assigned-task>.content-top>.searche-box>.search-ico{
  width: .13rem;
  height: .13rem;
  position: absolute;
  left: .3rem;
  top: .1rem;
  background: url("../images/search_ico.png") no-repeat center;
  background-size: 100% 100%;
}
.assigned-task>.content-top>.searche-box>.search{
  width: 100%;
  height: 100%;
  text-indent: .35rem;
  border: none;
  border-radius: 5px;
  background: #f9fafc;
  font-size: .13rem;
}
.assigned-task>.content-top>.content-tab{
  width: 100%;
  height: .38rem;
  line-height: .38rem;
  font-size: .15rem;
  font-weight: bold;
  color: #b0b0b0;
  background: #fff;
}
.assigned-task>.content-top>.content-tab>ul>li{
  width: 50%;
  float: left;
  text-align: center;
  display: inline-block;
}
.assigned-task>.content-top>.content-tab>ul>li>span{
  width: .87rem;
  line-height: .34rem;
  display: inline-block;
}
.assigned-task>.content-top>.content-tab>ul>li>span.active{
  border-bottom: .02rem solid #2679f7;
  color: #2679f7;
}
.assigned-task>.content-top>.content-tit{
  background: #f6f8fb;
}
.assigned-task>.content-top>.content-tit>p{
  width: 100%;
  height: .45rem;
  line-height: .45rem;
  padding: 0 0 0 .2rem;
  font-size: .13rem;
  font-weight: bold;
  color: #000;
}
.assigned-task>.content-top>.content-tit>.select-box{
  width: 100%;
  height: .38rem;
  line-height: .38rem;
  padding: 0 0 0 .2rem;
  font-size: .13rem;
  color: #000;
  background: #f6f8fb;
}
.assigned-task>.content-top>.content-tit>.select-box>.swiper-container>.swiper-wrapper>.swiper-slide{
  width: .75rem;
  height: .34rem;
  line-height: .34rem;
  font-size: .13rem;
  text-align: center;
  color: #434343;
  display: inline-block;
  border: .02rem solid #eeeff1;
  background: #fefefe;
  margin-right: .07rem;
}
.assigned-task>.content-top>.content-tit>.select-box>.swiper-container>.swiper-wrapper>span.active{
  border: .02rem solid #2679f7;
  background: #e8f1ff;
  background: url("../images/tab_bg.png") no-repeat center;
  background-size: 100% 100%;
}
.assigned-task>.content-bottom{
  position: relative;
  width: 100%;
  height: calc(100% - 2.2rem);
  /* margin-top: 2.1rem; */
  overflow-y: scroll;
  background: #f6f8fb;
}
.assigned-task>.content-bottom>.content-list{
  width: 100%;
  padding: .02rem .2rem .2rem 0.2rem;
  position: relative;
}
.assigned-task>.content-bottom>.content-list>.content-item{
  width: 100%;
  background: #fff;
  height: 1.1rem;
  margin: .2rem 0 0 0;
  padding: .15rem .15rem;
  border-radius: .1rem;
}
.assigned-task>.content-bottom>.content-list>.content-item>img{
  width: 34.5%;
  box-sizing: border-box;
  height: 100%;
  border-radius: .1rem;
  float: left;
}
.assigned-task>.content-bottom>.content-list>.content-item>div.item-right{
  box-sizing: border-box;
  width: 65.5%;
  height: 100%;
  float: right;
  padding: 0 0 0 .16rem;
}
.assigned-task>.content-bottom>.content-list>.content-item>div.noim{
  background: url("../images/noim.png") no-repeat right bottom;
  background-size: .485rem .46rem;
}
.assigned-task>.content-bottom>.content-list>.content-item>div.im{
  background: url("../images/im.png") no-repeat right bottom;
  background-size: .485rem .46rem;
}
.assigned-task>.content-bottom>.content-list>.content-item>div.item-right>span.type{
  float: right;
  width: .64rem;
  height: .24rem;
  line-height: .24rem;
  background: #fff7f6;
  color: #f5806c;
  font-size: .12rem;
  text-align: center;
  border-radius: .05rem;
}
.assigned-task>.content-bottom>.content-list>.content-item>div.item-right>span.tit{
  width: calc(100% - .64rem);
  display: inline-block;
  height: .24rem;
  line-height: .24rem;
  color: #333;
  font-size: .17rem;
  font-weight: 600;
  text-align: left;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.assigned-task>.content-bottom>.content-list>.content-item>div.item-right>span.sub{
  width: 100%;
  display: inline-block;
  height: .32rem;
  line-height: .16rem;
  color: #666;
  font-size: .12rem;
  text-align: left;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
}
.assigned-task>.content-bottom>.content-list>.content-item>div.item-right>span.time{
  color: #999;
  font-size: .1rem;
}
/* 交办详情 */
.assigned-task>.content-bottom>.content-list>.content-item2{
  width: 100%;
  background: #fff;
  margin: .2rem 0 0 0;
  padding: 0 .15rem;
  border-radius: .1rem;
}
.assigned-task>.content-bottom>.content-list>.content-item2>div.info{
